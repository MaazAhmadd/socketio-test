{
	"$schema": "https://biomejs.dev/schemas/1.8.3/schema.json",
	"organizeImports": {
		"enabled": true
	},
	"linter": {
		"enabled": true,

		"rules": {
			"recommended": true,
			"suspicious": {
				"noDebugger": "off",
				"noConsoleLog": "off",
				"noExplicitAny": "info"
			},
			"complexity": {
				"noForEach": "off"
			},
			"style": {
				"noShoutyConstants": "warn",
				"useNamingConvention": "off",
				"useImportType": "off"
			}
		}
	}
}
